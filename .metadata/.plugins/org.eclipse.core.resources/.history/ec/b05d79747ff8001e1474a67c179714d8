	<div>
		<xmp class="code"> 
		  for ~ of 반복문 : array, set, string, map 반복 가능한 객체를
		  				  반복적으로 처리하는 문
		  for(let/const 변수명 of 반복객체) {
		  }
		</xmp>
	</div>

	<script>
    // for in 반복문
    // *** for of 반복문 ***
    // [https://developer.mozilla.org/ko/docs/Web/JavaScript/Reference/Statements/for...of]
    const array1 = ['a', 'b', 'c'];
    console.log ( ) ;
    
    let iterable = "boo";

	for (let value of iterable) {
	  console.log(value);
	}
	// "b"
	// "o"
	// "o"
	
	
	let iterable = new Map([
	  ["a", 1],
	  ["b", 2],
	  ["c", 3],
	]);
	
	for (let entry of iterable) {
	  console.log(entry);
	}
	// [a, 1]
	// [b, 2]
	// [c, 3]
	
	for (let [key, value] of iterable) {
	  console.log(value);
	}
	// 1
	// 2
	// 3
	
	let iterable = new Set([1, 1, 2, 2, 3, 3]);

	for (let value of iterable) {
	  console.log(value);
	}
	// 1
	// 2
	// 3
	
	
	
	  </script>

