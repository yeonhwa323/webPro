  <script>
    // person 객체 안에
    const person = {
        // 속성
    	firstname: "길동",
    	lastname: "홍",
    	age: 20,
    	// 메서드
    	print: function(){
    		return this.firstname+"/"+this.age;
    	},
    	// 중첩된 객체 삽입 가능
    	//cars: { car1:"BMW", car2:"KIA"}
    	
    	// 중첩 배열 + 객체
    	cars: [
    		{ name:"k9", year:2020 },
    		{ name:"k5", year:2021}
    	]
    };
    
    // 1. 기존 객체에 새로운 속성을 추가(가능)
    person.eyeColor = "blue";
    
    // 2. 객체의 속성을 삭제
    delete person.age;
    alert( person.age ) ;  // undefined
