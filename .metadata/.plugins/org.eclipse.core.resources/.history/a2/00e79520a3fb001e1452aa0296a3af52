  <button onclick="windowOpen();">새 창 열기</button>
  <button onclick="windowClose();">창 닫기</button>
  
</div>

<script>
  let handle; // undefined , 0, null 모두 false
  function windowOpen(){
	  if ( !handle ) {
		  const windowFeatures = "left=100,top=200,width=600,height=500";
		  handle = window.open(
				  "ex08_02.html"
				  , ""
				  , windowFeatures);
	  } else {
		  handle.focus();
	  }
	  
	  handle = true;
	  
  }
  function windowClose(){
	  handle.close();
  }
</script>

<script>
  let name;
  function test() {}
  
  // [window.]name
  // [window.]test()
  // [window.]document.write();
  // [window.]alert();
  
</script>

