  <script>
    /* js Math 객체 */
    /*
    console.log( Math.random() );  
    Math.abs();
    Math.max();
    Math.min();
    Math.floor();
    Math.round();
    Math.ceil();
    */
    
    function getRndInt(min,max) {
    	// 0 ~ max-min
    	// 0.0 <= Math.random() < 1.0
    	// min <= Math.floor( Math.random()*(max-min+1)) <= max
    	return Math.floor( Math.random()*(max-min+1)) +min;
    };
    //getRndInt(1,45);
    
    $("button").on("click", function(event){
       const lotto = [];
       let n;
       while( lotto.length < 6 ){
    	   n = getRndInt(1,45);
       }//while
    });
  </script>
