  </xmp>
  
  <button id="go">Go</button>
  <button id="stop">Stop!</button>
  <button id="back">Back</button>
  <button id="slideToggle">slideToggle()</button>
  
  <div class="block"></div>
  
</div>

 <script>
   $("#go").click(function(){
	  $(".block")
	   	 .stop(true, false)
	   	 .animate({ left: "+=100px" }, 5000);	   
   });
   $("#stop").click(function(){
	   $(".block")
  		.stop(true, false);	   
   });
   $("#back").click(function(){
	   $(".block")
  		.stop(true, false)
  		.animate({ left: "-=100px" }, 5000);
   });
   $("#slideToggle").click(function(){
	   $(".block")
  		.stop(true, false)
  		.slideToggle(5000);
   }); 
 </script>


</body>
